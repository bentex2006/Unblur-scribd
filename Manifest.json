{
  "name": "Disblur By B3x",
  "description": "Show the blurred text in Scribd documents.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.scribd.com/*"],
      "css": ["disblur.css"]
    }
  ]
}
